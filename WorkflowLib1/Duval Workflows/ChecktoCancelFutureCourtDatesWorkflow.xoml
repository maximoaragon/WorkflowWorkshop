<ns0:BaseWorkflow Initialized="BaseWorkflow_Initialized" Completed="BaseWorkflow_Completed" InputParameters="{p1:Null}" x:Name="ChecktoCancelFutureCourtDatesWorkflow" ParameterValues="{x:Null}" InputParametersFormat="dispositionDate:System.DateTime" Case="{x:Null}" EnableCaseTransaction="False" xmlns="http://schemas.microsoft.com/winfx/2006/xaml/workflow" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:p1="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:ns0="http://aptitudesolutions.com/ShowCaseWorkflowActivityLib">
