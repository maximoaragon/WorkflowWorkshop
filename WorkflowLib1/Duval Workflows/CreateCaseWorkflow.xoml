<ns0:BaseWorkflow Initialized="BaseWorkflow_Initialized" Completed="BaseWorkflow_Completed" InputParameters="{p1:Null}" x:Name="CreateCaseWorkflow" ParameterValues="{x:Null}" InputParametersFormat="{x:Null}" Case="{x:Null}" EnableCaseTransaction="False" xmlns="http://schemas.microsoft.com/winfx/2006/xaml/workflow" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:p1="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:ns0="http://aptitudesolutions.com/ShowCaseWorkflowActivityLib">
	<ns4:CallMethodActivity MethodName="GetByProgramControl" ParameterTypes="{x:Null}" Type="{x:Type p5:CCaseStatuss}" x:Name="GetOpenCaseStatus" xmlns:p5="http://aptitudesolutions.com/ShowCaseClassLib" xmlns:ns4="http://aptitudesolutions.com/ShowCaseWorkflowActivityLib">
